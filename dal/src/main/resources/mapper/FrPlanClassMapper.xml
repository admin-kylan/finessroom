<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yj.dal.dao.FrPlanClassMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yj.dal.model.FrPlanClass">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="diff" property="diff" />
        <result column="image" property="image" />
        <result column="time" property="time" />
        <result column="strength" property="strength" />
        <result column="count" property="count" />
        <result column="remark" property="remark" />
        <result column="action_type" property="actionType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        name, diff, image, time, strength, count, remark, action_type AS actionType, id
    </sql>

</mapper>
